t grid;\nlet cols;\nlet rows;\nlet resolution = 20;\n\nconst s = (sketch) => {\n    sketch.setup = () => {\n        sketch.createCanvas(600, 400);\n        cols = sketch.width / resolution;\n        rows = sketch.height / resolution;\n\n        grid = new Matrix(cols, rows);\n        \n        for (let i = 0; i < cols; i++) {\n            for (let j = 0; j < rows; j++) {\n                grid[i][j] = sketch.floor(sketch.random(2));\n            }\n        }\n        console.table(grid);\n        \n    };\n\n    sketch.draw = () => {\n        sketch.background(0);\n        // creating another matrix that represents the new/next generation\n        let next = new Matrix(cols, rows);\n\n        for(let i = 0; i < cols; i++){\n            for(let j = 0; j < rows; j++){\n                let x = i * resolution;\n                let y = j * resolution;\n                \n                if (grid[i][j] == 1) {\n                    sketch.fill(255);\n                    sketch.stroke(0)\n                    sketch.rect(x, y, resolution-1, resolution-1);\n                }\n            }\n        }\n    };\n}\n\n\n\nlet myP5 = new p5(s)\n"},"lineCount":null}},"error":null,"hash":"f19b2dbb5192f16296934c97fff7a4a3","cacheData":{"env":{}}}